void keyPressed()
{
  switch(key)
  {
    case '1':     
      gnColorMode = 1;
      break;
    
    case '2':
      
      gnColorMode = 2;
      break;
      
      case '3':
      
      gnColorMode = 3;
      break;
      
      case '4':
      
      gnColorMode = 4;
      break;
      
      case '5':
      gnColorMode = 5;
      break;
      
    default:
      break;
  }
}
